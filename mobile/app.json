{
  "expo": {
    "name": "FixSleep",
    "slug": "fixsleep",
    "version": "1.1.4",
    "orientation": "portrait",
    "icon": "./assets/icons/ios-light.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.227faddi.fixsleep",
      "icon": {
        "light": "./assets/icons/ios-light.png",
        "dark": "./assets/icons/ios-dark.png",
        "tinted": "./assets/icons/ios-tinted.png"
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "permissions": [
        "WAKE_LOCK"
      ],
      "adaptiveIcon": {
        "monochromeImage": "./assets/icons/adaptive-icon.png",
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.x227faddi.fixsleep"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/icons/ios-light.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/icons/splash-icon-light.png",
            "backgroundColor": "#2A2A2E"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/notification-icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "expo-localization",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Fredoka-Regular.ttf",
            "./assets/fonts/Fredoka-Medium.ttf",
            "./assets/fonts/Fredoka-Bold.ttf"
          ]
        }
      ],
      [
        "expo-av",
        {
          "androidImplementation": "EXOPLAYER"
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e2685869-08ba-4e2a-a23c-375cfe1e4752"
      }
    }
  }
}
